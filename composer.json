{
    "name":         "alcalyn/serializable-api-response",
    "license":      "MIT",
    "type":         "library",
    "description":  "Allows to return raw object as controller response so that it will be serialized later by serializer and converted to Symfony Response.",
    "homepage":     "https://github.com/alcalyn/serializable-api-response",
    "keywords":     ["serializer", "symfony"],
    "authors": [
        {
            "name": "Julien Maulny",
            "email": "doubjulien@hotmail.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "jms/serializer": "~1.1",
        "symfony/http-foundation": "~2.0|~3.0",
        "symfony/http-kernel": "~2.0|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Alcalyn\\SerializableApiResponse\\": "src/"
        }
    }
}
